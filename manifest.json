{
    "manifest_version":2,
    "name": "Bookmark Extension",
    "description": "An extension that keeps track of where you are on a page.",
    "version": "1",
    "browser_action": {
        "default_title": "Where was I?",
        "default_popup":"popup.html",
        "default_popup":"Add_page.html",
        "default_popup":"Tabs.html",
        "default_popup":"View_page.html",
        "default_icon": "icon.png"
    },
    "permissions": ["activeTab", "bookmarks"]
}